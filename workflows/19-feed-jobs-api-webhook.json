    {
      "parameters": {
        "jsCode": "const inputItems = $input.all();\nconst jobs = inputItems.flatMap(item => {\n  const data = item.json;\n  return Array.isArray(data) ? data : [data];\n}).filter(job => job && job.id);\n\nconst request = $('validate-request').first().json;\nconst profile = $('extract-profile').first().json;\nconst isPro = profile.is_pro;\n\n// Format jobs based on tier\nconst formattedJobs = jobs.map(job => {\n  const baseJob = {\n    id: job.id,\n    title: job.title,\n    company_name: job.company_name,\n    company_id: job.company_id,\n    location_city: job.location_city,\n    location_type: job.location_type,\n    salary_raw: job.salary_raw,\n    seniority_level: job.seniority_level,\n    posted_at: job.posted_at,\n    description_summary: job.description_summary,\n    match_label: job.match_label,\n    is_new: job.is_new\n  };\n\n  if (isPro) {\n    return {\n      ...baseJob,\n      salary_min: job.salary_min,\n      salary_max: job.salary_max,\n      match_score: job.match_score,\n      match_insights: job.match_insights,\n      required_skills: job.required_skills,\n      benefits: job.benefits\n    };\n  }\n  \n  return baseJob;\n});\n\n// Calculate pagination\nconst hasMore = jobs.length === request.limit;\nconst nextOffset = hasMore ? request.offset + request.limit : null;\n\nreturn [{\n  json: {\n    status: 'success',\n    data: {\n      jobs: formattedJobs,\n      pagination: {\n        limit: request.limit,\n        offset: request.offset,\n        count: jobs.length,\n        has_more: hasMore,\n        next_offset: nextOffset\n      }\n    },\n    meta: {\n      tier: profile.subscription_tier,\n      filters_applied: request.filters\n    }\n  }\n}];"
      },
