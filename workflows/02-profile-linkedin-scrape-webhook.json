    {
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "const clado = $input.item.json;\nconst p = clado.profile || {};\n\n// Parse location\nlet city = '', country = '', locationRaw = p.location || '';\nif (locationRaw) {\n  const parts = locationRaw.split(',').map(s => s.trim());\n  city = parts[0] || '';\n  country = parts[parts.length - 1] || '';\n}\n\n// Build markdown\nconst sections = [`# ${p.name || 'LinkedIn Profile'}\\n`];\nif (p.headline) sections.push(`**${p.headline}**\\n`);\nif (p.location) sections.push(`\ud83d\udccd ${p.location}\\n`);\n\n// Experience\nif (clado.experience?.length) {\n  sections.push('\\n## Experience\\n');\n  clado.experience.forEach(exp => {\n    sections.push(`### ${exp.title} at ${exp.company}`);\n    if (exp.dates) sections.push(`*${exp.dates}*`);\n    if (exp.description) sections.push(exp.description);\n    sections.push('');\n  });\n}\n\n// Education\nif (clado.education?.length) {\n  sections.push('\\n## Education\\n');\n  clado.education.forEach(edu => {\n    sections.push(`- **${edu.degree || edu.school}** ${edu.school ? `at ${edu.school}` : ''} ${edu.year ? `(${edu.year})` : ''}`);\n  });\n}\n\n// Skills\nif (clado.skills?.length) {\n  sections.push('\\n## Skills\\n');\n  sections.push(clado.skills.join(' \u2022 '));\n}\n\nreturn {\n  json: {\n    markdown: sections.join('\\n'),\n    extracted: {\n      full_name: p.name,\n      headline: p.headline,\n      profile_photo_url: p.photo_url,\n      linkedin_profile_id: p.member_id,\n      location_city: city,\n      location_country: country,\n      location_raw: locationRaw\n    }\n  }\n};"
      },
